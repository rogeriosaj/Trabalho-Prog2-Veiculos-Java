<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ExcluirVeiculoController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Menu">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#voltarAoMenu" text="Voltar ao Menu" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="257.0" layoutY="65.0" prefHeight="27.0" prefWidth="201.0" text="Exclusão de Veículo " textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Label layoutX="116.0" layoutY="144.0" text="Selecione o veículo" />
                <Button layoutX="247.0" layoutY="210.0" mnemonicParsing="false" onAction="#voltarAoMenu" prefHeight="26.0" prefWidth="83.0" text="Cancelar" />
                <Button layoutX="385.0" layoutY="210.0" mnemonicParsing="false" onAction="#excluirVeiculo" prefHeight="26.0" prefWidth="83.0" text="Excluir" />
                <ComboBox fx:id="comboVeiculos" layoutX="247.0" layoutY="139.0" prefHeight="26.0" prefWidth="222.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
