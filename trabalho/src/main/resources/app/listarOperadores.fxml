<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ListarOperadoresController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>            
                <Menu mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu" />
                    </items>
                </Menu>
            </menus>
        
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="500.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="205.0" layoutY="41.0" text="Lista de Operadores">
                    <font>
                        <Font size="26.0" />
                    </font>
                </Label>
            <ListView fx:id="listaOperadores" layoutX="220.0" layoutY="88.0" prefHeight="200.0" prefWidth="200.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
